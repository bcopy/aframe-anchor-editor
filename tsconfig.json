{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "sourceMap": true,
    "types": ["aframe"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}